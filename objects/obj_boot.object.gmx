        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Physics

var rkey = keyboard_check(vk_right);
var lkey = keyboard_check(vk_left);

Left = virtual_key_add(0, 0, display_get_gui_width()/2, display_get_gui_height(), vk_left);
Right = virtual_key_add(display_get_gui_width()/2, 0, display_get_gui_width()/2, display_get_gui_height(), vk_right);

//check for ground
if (place_meeting(x, y+1, obj_solid))

{
    vspd = -jspd;
    //audio_play_sound(jump_sound, 3, false);
    if (hspd != 0)
    {
        if (hspd &lt; 0)
        {
        hspd += fric;
        }
        else
        {
            hspd -= fric;
        }
    }
}  
    
else  
    {
        vspd += grav;
        if vspd &gt;= 25
        {
            vspd = 25;
        }        
    }
    
//Controlling Horizontal Movement
if (rkey or Right)
{
    if (hspd &lt; spd) 
    {
        hspd+=fric;
    }
    else
    {
        hspd = spd;
    }
}
if (lkey or Left)
{
    if (hspd &gt; -spd) 
    {
        hspd-=fric;
    }
    else
    {
        hspd = -spd;
    }
}

/*//Check for Not Moving
if ((!rkey &amp;&amp; !lkey) || (rkey &amp;&amp; lkey))
{
    if (hspd != 0)
    {
        if (hspd &lt; 0)
        {
        hspd += fric;
        }
        else
        {
            hspd -= fric;
        }
    }
}*/

//Horizontal Collisions
if (place_meeting(x+hspd, y, obj_solid))
{
    while(!place_meeting(x+sign(hspd), y, obj_solid))
    {
        x += sign(hspd);
    }
    hspd = 0;
}
    
//Move Horizontally
x += hspd;

//vertical collisions
if (place_meeting(x, y+vspd, obj_solid))
{
    while(!place_meeting(x, y+sign(vspd), obj_solid))
    {
        y += sign(vspd);
    }
    vspd = 0;
}

//Move Vertically
y += vspd;

//Control Sprites
if xprevious != x and vspd &lt; -8 //moving and going up
{
    sprite_index = spr_player_hop;
    image_index = 1;   
}
else if xprevious != x and vspd &gt; 4 //moving and apex
{
    sprite_index = spr_player_hop;
    image_index = 2;   
}
else //moving and going down (stays this way until you land)
{
    sprite_index = spr_player_stand;
    image_speed = 0;
}
//Sprite Direction
if x &gt; xprevious
{
    image_xscale = 1;
}
if x &lt; xprevious
{
    image_xscale = -1;
}

//Death (for now)
if (dead = true)
{
    instance_change(obj_boot_dead, true);
    audio_play_sound(explode_sound, 3, false); 
}
</string>
